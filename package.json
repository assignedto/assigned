{
  "name": "assigned",
  "version": "1.0.0",
  "description": "Assigned - See what issues await you.",
  "main": "main.js",
  "build": {
    "appId": "com.assignedto.assigned",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "assignedto",
        "repo": "assigned"
      }
    ]
