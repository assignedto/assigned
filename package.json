    "electron-contextmenu-middleware": "1.0.3",
    "electron-fetch": "1.3.0",
    "electron-input-menu": "2.1.0",
    "electron-is-dev": "1.1.0",
    "electron-oauth-helper": "5.0.1",
    "electron-store": "5.0.0",
    "electron-updater": "4.1.2",
    "menubar": "6.0.8",
    "moment": "2.24.0",
    "query-string": "6.8.3"
